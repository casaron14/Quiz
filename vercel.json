{
    "version": 2,
    "buildCommand": "echo 'No build needed'",
    "outputDirectory": ".",
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}